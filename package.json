    "@types/jest": "^29.5.1",
    "@types/mocha": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "should": "^13.2.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typemoq": "^2.1.0",
    "typescript": "^5.0.4"
